{
	"name": "sort-obj",
	"version": "0.0.0",
	"description": "Sort the keys and values of an object.",
	"repository": "https://github.com/Richienb/sort-obj.git",
	"author": "Richie Bendall <richiebendall@gmail.com>",
	"license": "MIT",
	"main": "index.js",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"lint": "xo",
		"test": "xo && ava"
	},
	"keywords": [
		"sort",
		"object",
		"map",
		"function",
		"modify",
		"change",
		"key",
		"value"
	],
	"dependencies": {
		"fromentries": "^1.2.0",
		"type-fest": "^0.16.0"
	},
	"devDependencies": {
		"alpha-sort": "^3.1.0",
		"ava": "^3.9.0",
		"eslint-config-richienb": "^0.4.2",
		"num-sort": "^2.1.0",
		"xo": "^0.32.0"
	},
	"xo": {
		"extends": "richienb",
		"rules": {
			"import/no-unresolved": [
				"error",
				{
					"ignore": [
						"type-fest"
					]
				}
			],
			"no-unused-vars": [
				"error",
				{
					"vars": "all",
					"args": "after-used",
					"ignoreRestSiblings": true,
					"argsIgnorePattern": "^_",
					"caughtErrors": "all",
					"caughtErrorsIgnorePattern": "^_"
				}
			]
		}
	}
}
